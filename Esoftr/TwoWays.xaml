<Window x:Class="Esoftr.TwoWays"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Esoftr"
        mc:Ignorable="d"
        Title="Выбор" Height="300" Width="300" FontFamily="Calibri" Icon="logo.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button x:Name="tttask" Content="Задачи" Grid.Row="0" Width="120" Height="40" VerticalAlignment="Center" Background="#FF00D9BB" BorderThickness="1" BorderBrush="#FF37474F" Foreground="#FF243238" Click="tttask_Click"/>
        <Button x:Name="eeexec" Content="Исполнители" Grid.Row="1" Width="120" Height="40" VerticalAlignment="Center" Background="#FF00D9BB" BorderThickness="1" BorderBrush="#FF37474F" Foreground="#FF243238" Click="eeexec_Click"/>
    </Grid>
</Window>
